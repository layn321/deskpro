{% set font_headings = 'Helvetica,arial,freesans,clean,sans-serif' %}
{% set font_body = '"Helvetica Neue", Helvetica, Arial, sans-serif' %}

{% macro gradient(start_color, end_color, fallback_color, height) %}
	{% if not fallback_color %}{% set fallback_color = start_color %}{% endif %}
	background: {{ fallback_color }};
	background: -webkit-gradient(linear, left bottom, left top, color-stop(0, {{ start_color }}), color-stop(1, {{ end_color }}));
	background: -ms-linear-gradient(bottom, {{ start_color }}, {{ end_color }});
	background: -moz-linear-gradient(center bottom, {{ end_color }} 0%, {{ start_color }} 100%);
{% endmacro %}

{% macro border_radius(options) %}
	{% if not is_array(options) %}
		{% set options = { tr: options, br: options, bl: options, tl: options } %}
	{% endif %}
	{% if options.tr is not null %}-webkit-border-top-right-radius: {{ options.tr }}px;{% endif %}
	{% if options.br is not null %}-webkit-border-bottom-right-radius: {{ options.br }}px;{% endif %}
	{% if options.bl is not null %}-webkit-border-bottom-left-radius: {{ options.bl }}px;{% endif %}
	{% if options.tl is not null %}-webkit-border-top-left-radius: {{ options.tl }}px;{% endif %}
	{% if options.tr is not null %}-moz-border-radius-topright: {{ options.tr }}px;{% endif %}
	{% if options.br is not null %}-moz-border-radius-bottomright: {{ options.br }}px;{% endif %}
	{% if options.bl is not null %}-moz-border-radius-bottomleft: {{ options.bl }}px;{% endif %}
	{% if options.tl is not null %}-moz-border-radius-topleft: {{ options.tl }}px;{% endif %}
	{% if options.tr is not null %}border-top-right-radius: {{ options.tr }}px;{% endif %}
	{% if options.br is not null %}border-bottom-right-radius: {{ options.br }}px;{% endif %}
	{% if options.bl is not null %}border-bottom-left-radius: {{ options.bl }}px;{% endif %}
	{% if options.tl is not null %}border-top-left-radius: {{ options.tl }}px;{% endif %}
{% endmacro %}

{% macro box_shadow(args) %}
	-webkit-box-shadow: {{ args }};
	-moz-box-shadow: {{ args }};
	box-shadow: {{ args }};
{% endmacro %}

{% macro opacity(op) %}
	opacity: {{ op }};
	filter:alpha(opacity={{ op * 100 }}); /* For IE8 and earlier */
{% endmacro %}

