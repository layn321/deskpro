#dp .dp-accordion{margin-bottom:18px;}
#dp .dp-accordion-group{margin-bottom:2px;border:1px solid #e5e5e5;-webkit-border-radius:4px;-moz-border-radius:4px;border-radius:4px;}
#dp .dp-accordion-heading{border-bottom:0;}
#dp .dp-accordion-heading .dp-accordion-toggle{display:block;padding:8px 15px;}
#dp .dp-accordion-inner{padding:9px 15px;border-top:1px solid #e5e5e5;}
#dp .dp-carousel{position:relative;margin-bottom:18px;line-height:1;}
#dp .dp-carousel-inner{overflow:hidden;width:100%;position:relative;}
#dp .dp-carousel .dp-item{display:none;position:relative;-webkit-transition:0.6s ease-in-out left;-moz-transition:0.6s ease-in-out left;-ms-transition:0.6s ease-in-out left;-o-transition:0.6s ease-in-out left;transition:0.6s ease-in-out left;}
#dp .dp-carousel .dp-item>img{display:block;line-height:1;}
#dp .dp-carousel .dp-active,#dp .dp-carousel .dp-next,#dp .dp-carousel .dp-prev{display:block;}
#dp .dp-carousel .dp-active{left:0;}
#dp .dp-carousel .dp-next,#dp .dp-carousel .dp-prev{position:absolute;top:0;width:100%;}
#dp .dp-carousel .dp-next{left:100%;}
#dp .dp-carousel .dp-prev{left:-100%;}
#dp .dp-carousel .dp-next.dp-left,#dp .dp-carousel .dp-prev.dp-right{left:0;}
#dp .dp-carousel .dp-active.dp-left{left:-100%;}
#dp .dp-carousel .dp-active.dp-right{left:100%;}
#dp .dp-carousel-control{position:absolute;top:40%;left:15px;width:40px;height:40px;margin-top:-20px;font-size:60px;font-weight:100;line-height:30px;color:#ffffff;text-align:center;background:#222222;border:3px solid #ffffff;-webkit-border-radius:23px;-moz-border-radius:23px;border-radius:23px;opacity:0.5;filter:alpha(opacity=50);}
#dp .dp-carousel-control.dp-right{left:auto;right:15px;}
#dp .dp-carousel-control:hover{color:#ffffff;text-decoration:none;opacity:0.9;filter:alpha(opacity=90);}
#dp .dp-carousel-caption{position:absolute;left:0;right:0;bottom:0;padding:10px 15px 5px;background:#333333;background:rgba(0, 0, 0, 0.75);}
#dp .dp-carousel-caption h4,#dp .dp-carousel-caption p{color:#ffffff;}
#dp .dp-hero-unit{padding:60px;margin-bottom:30px;background-color:#eeeeee;-webkit-border-radius:6px;-moz-border-radius:6px;border-radius:6px;}
#dp .dp-hero-unit h1{margin-bottom:0;font-size:60px;line-height:1;color:inherit;letter-spacing:-1px;}
#dp .dp-hero-unit p{font-size:18px;font-weight:200;line-height:27px;color:inherit;}
#dp .dp-pull-right{float:right;}
#dp .dp-pull-left{float:left;}
#dp .dp-hide{display:none;}
#dp .dp-show{display:block;}
#dp .dp-invisible{visibility:hidden;}