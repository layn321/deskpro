<!DOCTYPE HTML>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="robots" content="NOINDEX" />
	<meta http-equiv="X-UA-Compatible" content="IE=100" />

	{% include 'AgentBundle:Main:resources.html.twig' %}

	<style type="text/css">
	body, html {
		background: #fff;
		min-height: inherit;
		min-width: inherit;
	}
	</style>

	<script type="text/javascript">
	if (opener && opener.DeskPRO_Window) {
		var DeskPRO_Window = opener.DeskPRO_Window;
	} else if (parent && parent.DeskPRO_Window) {
		var DeskPRO_Window = parent.DeskPRO_Window;
	} else {
		var DeskPRO_Window = null;
	}
	</script>

	{% block head %}{% endblock %}
</head>
<body>

<div class="overlay-title">
	{% block header %}{% endblock %}
</div>
<div class="overlay-content no-footer">
	{% block body %}{% endblock %}
</div>

</body>
</html>